{
    "submit" : {
        "background": "rgb(52,101,164)",
        "color": "#fff",
        "text-shadow": "1px 1px 1px #222",
        "border": "1px solid #555",
        "font-weight": "bold",
        "font-size": "85%",
        "text-decoration": "none",
        "padding": "0.5em 1em",
        "cursor":"pointer",
        "border-radius": "2px",
        "border": "1px solid #aaa"
    }
    ,

    "headdiv" : {
        "background": "none repeat scroll 0 0 #3465a4",
        "height": "2.5em",
        "padding": "0"
    }
    ,

    "headbar" : {
        "margin": "0 auto",
        "max-width": "70em"
    }
    ,

    "headimg" : {
        "margin-top": "0.25em",
        "max-height": "40px"
    }
    ,

    "headlink" : {
        "text-decoration" : "none; border: none",
        "font-size" : "20px",
        "color" : "#EBA846",
        "line-height" : "35px"
    }
    ,

    "logo" : {
        "float": "left",
        "margin": "0 10px"
    }
    ,

    "body" : {
        "display": "table",
        "font-family": "Verdana,sans-serif",
        "font-size": "12pt",
        "line-height": "1.25em",
        "margin": "0 auto",
        "min-width": "100%",
        "padding": "0"
    }
    ,

    "bodywrap": {
        "background": "#fff"
    }
    ,

    "bodydiv" : {
        "color": "#111",
        "margin": "0 auto",
        "max-width": "70em",
        "padding": "0.5em 1em"
    }
    ,

    "footdiv" : {
        "border-top": "1px solid #bbb",
        "bottom": "0",
        "color": "#555555",
        "font-size": "70%",
        "margin-bottom": "0",
        "padding": "1em"
    },

    "footaddress" : {
        "font-style": "normal",
        "text-align": "center"
    }
}
